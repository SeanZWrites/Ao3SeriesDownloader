{
    "name": "Ao3 Series Downloader",
    "description": "Easily download a series from Ao3!",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["downloads"],
    "content_scripts" : [
        {
            "matches": ["https://archiveofourown.org/series/*"],
            "js": ["ao3SeriesDownloader.js"]
        }
    ],
    "background": {
        "scripts": ["backgroundDownloader.js"],
        "persistent": false
    }
  }